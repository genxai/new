{
  "name": "new",
  "private": true,
  "type": "module",
  "version": "0.8.0",
  "scripts": {
    "dev": "npm-run-all --parallel dev:frontend dev:backend",
    "dev:frontend": "vite",
    "dev:backend": "convex dev --typecheck-components --live-component-sources",
    "generate": "convex dev --once",
    "logs": "convex logs",
    "lint": "tsc -p convex && eslint convex",
    "setup": "node scripts/setup.mjs",
    "update-envs": "node scripts/update-envs.mjs",
    "prettier": "prettier -c .",
    "format": "prettier --write .",
    "build": "vite build",
    "check": "node scripts/guardrails.cjs && node scripts/consistency-check.mjs --report && tsc -p . --noEmit && eslint . --max-warnings=0 && prettier -c . && vitest run --reporter=verbose",
    "audit": "node scripts/guardrails.cjs && node scripts/consistency-check.mjs --report && npx secretlint \"**/*\" && node scripts/run-gitleaks.mjs detect --no-banner --redact --no-git -v && npm audit --audit-level=moderate --production",
    "s": "bun --watch scripts/i-run.ts"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.3",
    "@convex-dev/better-auth": "^0.8.5",
    "@convex-dev/crons": "^0.1.9",
    "@convex-dev/resend": "^0.1.13",
    "@hookform/resolvers": "^5.2.2",
    "@react-email/components": "^0.5.3",
    "@tailwindcss/vite": "^4.1.13",
    "@vapi-ai/server-sdk": "^0.10.2",
    "@vapi-ai/web": "^2.4.0",
    "@zxcvbn-ts/core": "^3.0.4",
    "ai": "^5.0.51",
    "better-auth": "1.3.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk-base": "^0.0.8",
    "convex": "^1.27.3",
    "convex-helpers": "^0.1.104",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-day-picker": "^9.10.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-phone-number-input": "^3.4.12",
    "react-resizable-panels": "^3.0.6",
    "react-router": "^7.9.1",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.1",
    "resend": "^6.1.0",
    "streamdown": "^1.3.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.13",
    "tw-animate-css": "^1.3.8",
    "vaul-base": "^0.0.6",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@secretlint/secretlint-rule-basicauth": "^11.2.4",
    "@secretlint/secretlint-rule-pattern": "^11.2.4",
    "@secretlint/secretlint-rule-preset-canary": "^11.2.4",
    "@secretlint/secretlint-rule-preset-recommend": "^11.2.4",
    "@secretlint/secretlint-rule-secp256k1-privatekey": "^11.2.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/common-tags": "^1.8.4",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@types/semver": "^7.7.1",
    "@vitejs/plugin-react": "^5.0.3",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "eslint": "^9.35.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "secretlint": "^11.2.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "vite": "^7.1.5",
    "vitest": "^3.2.4"
  },
  "prettier": {
    "semi": false
  }
}
